  "directions": [
    {
      "_id": "5974dae8db9b1e6dfd36a205",
      "step": "Preheat the oven to 350F."
    },
    {
      "_id": "5974daf6db9b1e6dfd36a206",
      "step": "Sift the flour, baking soda, and salt into a medium bowl, set aside."
    },
    {
      "_id": "5974daffdb9b1e6dfd36a207",
      "step": "Whisk the eggs and vanilla together in a liquid measuring cup with a spout, set aside."
    },
    {
      "_id": "5974db06db9b1e6dfd36a208",
      "step": "Lightly brush a 9 by 5 by 3-inch loaf pan with butter."
    },
    {
      "_id": "5974db10db9b1e6dfd36a209",
      "step": "In a standing mixer fitted with the paddle attachment or with an electric hand-held mixer, cream the butter and sugar until light and fluffy."
    },
    {
      "_id": "5974db18db9b1e6dfd36a20a",
      "step": "Gradually pour the egg mixture into the butter while mixing until incorporated."
    },
    {
      "_id": "5974db22db9b1e6dfd36a20b",
      "step": "Add the bananas (the mixture will appear to be curdled, so don't worry), and remove the bowl from the mixer."
    },
    {
      "_id": "5974db2bdb9b1e6dfd36a20c",
      "step": "With a rubber spatula, mix in the flour mixture until just incorporated. Fold in the nuts and transfer the batter to the prepared pan."
    },
    {
      "_id": "5974db32db9b1e6dfd36a20d",
      "step": "Bake for 55 minutes or until a toothpick inserted into the center of the bread comes out clean."
    },
    {
      "_id": "5974db3bdb9b1e6dfd36a20e",
      "step": "Cool the bread in the pan on a wire rack for 5 minutes."
    },
    {
      "_id": "5974db40db9b1e6dfd36a20f",
      "step": "Turn the bread out of the pan and let cool completely on the rack."
    }
  ],
