    {
      "_id": "5a49746a277bd9fe34f72bb6",
      "step": "Toss the kale, parsley and shallot with 2 tablespoons of the olive oil and let sit for 10 minutes."
    },
    {
      "_id": "5a4975e6277bd9fe34f72bb7",
      "step": "Puree the remaining 2 tablespoons of olive oil, the lemon juice, garlic, anchovies and 1 ounce of the feta in a food processor."
    },
    {
      "_id": "5a497604277bd9fe34f72bb8",
      "step": "Pour the dressing over the kale, toss in the preserved lemon, chickpeas and remaining feta."
    }
