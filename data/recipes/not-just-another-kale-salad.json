        "directions": [
          {
            "_id": "5a497284277bd9fe34f72ba3",
            "step": "Rinse the chickpeas under cold water. Dry well (salad spinner + paper towels, etc)."
          },
          {
            "_id": "5a4972ab277bd9fe34f72ba4",
            "step": "Toss the chickpeas with oil and salt. Place on a sheet pan."
          },
          {
            "_id": "5a4972c2277bd9fe34f72ba5",
            "step": "Put the pan in a cold oven and turn the oven to 350F."
          },
          {
            "_id": "5a4972da277bd9fe34f72ba6",
            "step": "Turn the oven off after 30 minutes and leave the chickpeas in the oven for another hour."
          },
          {
            "_id": "5a497305277bd9fe34f72ba7",
            "step": "Toss the chickpeas with sumac and cayenne. Cool completely before storing."
          }
        ],
