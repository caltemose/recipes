  "__v": 18,
  "directions": [
    {
      "_id": "5a402bfd9c5f02065d03f1bc",
      "step": "Heat oven to 375F."
    },
    {
      "_id": "5a402c059c5f02065d03f1bd",
      "step": "Spread rice into an 8-inch square glass baking dish. Stir in 1 TBS olive oil and 1 ts salt."
    },
    {
      "_id": "5a402c1b9c5f02065d03f1be",
      "step": "Bring 2.5 cups water to boil and pour over the rice. Cover tightly with aluminum foil and back for 1 hour."
    },
    {
      "_id": "5a402c369c5f02065d03f1bf",
      "step": "Place a 12-inch saut√© pan over medium heat. Add remaining 3 TBS olive oil and garlic and pepper flakes. Cook slightly, add onion and 1/2 ts salt and cook for 1-2 minutes, until the onion starts to color."
    },
    {
      "_id": "5a402c779c5f02065d03f1c0",
      "step": "Add black beans, oregano and black pepper. Cook, stirring frequently, for 6-8 minutes."
    },
    {
      "_id": "5a402c8a9c5f02065d03f1c1",
      "step": "Remove the cover from the rice and fluff with a fork."
    },
    {
      "_id": "5a402c989c5f02065d03f1c2",
      "step": "Serve the beans over the rice and garnish with avocado."
    }
  ],
