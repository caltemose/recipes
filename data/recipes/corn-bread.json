  "directions": [
    {
      "_id": "5a4034a28ab0e729e2e01ad2",
      "step": "Preheat oven to 375F. Grease an 8-inch square pan."
    },
    {
      "_id": "5a4034ad8ab0e729e2e01ad3",
      "step": "Melt butter in sauce pan. Remove from heat and stir in sugar. Add eggs and beat until well blended."
    },
    {
      "_id": "5a4034c48ab0e729e2e01ad4",
      "step": "Combine buttermilk with baking soda and stir mixture into pan."
    },
    {
      "_id": "5a4034d48ab0e729e2e01ad5",
      "step": "Combine remaining dry ingredients in bowl and mix."
    },
    {
      "_id": "5a4034f88ab0e729e2e01ad6",
      "step": "Combine wet and dry mixtures and mix."
    },
    {
      "_id": "5a4035058ab0e729e2e01ad7",
      "step": "Pour batter into the greased pan and bake for 30-40 minutes."
    }
  ],
